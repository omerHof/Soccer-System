    <JFXButton id="button" fx:id="login" layoutX="722.0" layoutY="348.0" onAction="#verifyDetails" prefHeight="37.0" prefWidth="129.0" styleClass="d" stylesheets="@allPages.css" text="Log In" textFill="#2c9e5b">
